zx_library("explicit-memory") {
  sdk = "static"
  sdk_headers = [ "explicit-memory/bytes.h" ]
  kernel = true
  static = true
  sources = [ "bytes.c" ]
  if (!is_kernel) {
    deps = [ "$zx/system/ulib/fbl" ]
  }
}
