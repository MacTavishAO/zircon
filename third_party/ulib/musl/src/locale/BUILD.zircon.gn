source_set("locale") {
  deps = [ "$zx/third_party/ulib/musl:musl_internal" ]
  sources = [
    "__lctrans.c",
    "__mo_lookup.c",
    "c_locale.c",
    "catclose.c",
    "catgets.c",
    "catopen.c",
    "duplocale.c",
    "freelocale.c",
    "iconv.c",
    "langinfo.c",
    "locale_map.c",
    "localeconv.c",
    "newlocale.c",
    "pleval.c",
    "setlocale.c",
    "strcoll.c",
    "strfmon.c",
    "strxfrm.c",
    "uselocale.c",
    "wcscoll.c",
    "wcsxfrm.c",
  ]

  configs -= [ "$zx_build/public/gn/config:visibility_hidden" ]
}
