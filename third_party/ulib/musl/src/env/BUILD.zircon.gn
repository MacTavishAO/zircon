source_set("env") {
  deps = [ "$zx/third_party/ulib/musl:musl_internal" ]
  sources = [
    "__environ.c",
    "__libc_start_main.c",
    "__stack_chk_fail.c",
    "clearenv.c",
    "getenv.c",
    "putenv.c",
    "setenv.c",
    "unsetenv.c",
  ]

  configs -= [ "$zx_build/public/gn/config:visibility_hidden" ]
}
