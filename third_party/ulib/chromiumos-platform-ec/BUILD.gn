config("headers.config") {
  visibility = [ ":*" ]
  include_dirs = [ "include" ]
}

group("headers") {
  public_configs = [ ":headers.config" ]
}

static_library("chromiumos-platform-ec") {
  sources = [ "comm-lpc.cc" ]
  public_deps = [ ":headers" ]
}
