library fidl.examples.types;

using zx;

struct this_is_a_struct {
    string s;
};

union this_is_a_union {
    1: string s;
};

flexible union this_is_a_xunion {
    1: string s;
    2: int32 i;
};

protocol this_is_a_protocol {
    Copy(string s, uint32 count) -> (string s);
};

struct protocols_and_requests {
    this_is_a_protocol nonnullable_protocol;
    this_is_a_protocol? nullable_protocol;
    request<this_is_a_protocol> nonnullable_request;
    request<this_is_a_protocol>? nullable_request;
};

