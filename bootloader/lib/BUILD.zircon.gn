assert(toolchain.environment == "efi")

source_set("lib") {
  sources = [
    "cmdline.c",
    "console-printf.c",
    "ctype.c",
    "efi/guids.c",
    "inet.c",
    "loadfile.c",
    "printf.c",
    "stdlib.c",
    "string.c",
    "strings.c",
    "utf_conversion.c",
    "xefi.c",
  ]
  deps = [
    "$zx/kernel/lib/efi",

    # This only actually adds anything to the profile build.
    "$zx/kernel/lib/instrumentation:profile",
  ]
}
